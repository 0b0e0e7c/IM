type (
	UserRequest {
		Username string `json:"username"`
		Password string `json:"password"`
	}
	UserResponse {
		Id       int64  `json:"id"`
		Username string `json:"username"`
	}
)

service user-api {
	@handler Register
	post /api/user/register (UserRequest) returns (UserResponse)

	@handler Login
	post /api/user/login (UserRequest) returns (UserResponse)
}

